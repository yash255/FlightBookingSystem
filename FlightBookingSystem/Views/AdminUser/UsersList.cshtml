@model List<FlightBookingSystem.Models.User>

<h2>User List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
           
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                
                <td>
                    <a href="@Url.Action("EditUser", "AdminUser", new { id = user.Id })">Edit</a> |
                    <a href="@Url.Action("DeleteUser", "AdminUser", new { id = user.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

